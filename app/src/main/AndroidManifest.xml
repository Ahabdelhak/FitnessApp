<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ah_abdelhak.fitnessapp">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListExercises" />
        <activity android:name=".view_Exercise" />
        <activity android:name=".SettingPage" />

        <receiver android:name=".AlarmNotificationReceiver" />

        <activity android:name=".Daily_Training" />
        <activity android:name=".Calendar"></activity>
    </application>

</manifest>